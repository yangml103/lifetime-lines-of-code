{
  "name": "lifetime-lines-of-code",
  "displayName": "Lifetime Lines of Code",
  "description": "Shows you how many lines of code you've written in a specific language to make job applications easier",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/yangml103/lifetime-lines-of-code"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lifetime-lines-of-code.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "lifetime-lines-of-code.countLines",
        "title": "Count Lines"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
